    "import os\n",
    "\n",
    "import ipywidgets as widgets\n",
    "import meshio\n",
    "import numpy as np\n",
    "import plotly.graph_objects as go\n",
    "import tetgen as tg\n",
    "import trimesh\n",
    "from IPython.display import display, clear_output\n",
    "from noise import pnoise3"
   ]
  },
  {
   "cell_type": "code",
